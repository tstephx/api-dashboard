{
  "name": "@tstephx/api-dashboard",
  "version": "0.1.0",
  "description": "React components for API usage monitoring dashboards",
  "license": "MIT",
  "author": "Taylor Stephens",
  "repository": {
    "type": "git",
    "url": "https://github.com/tstephx/api-dashboard.git"
  },
  "keywords": ["react", "dashboard", "api", "monitoring", "typescript"],
  "files": ["dist"],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
